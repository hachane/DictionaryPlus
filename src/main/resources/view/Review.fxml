<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="560" prefWidth="750.0" styleClass="main-container" stylesheets="@../style/main1.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.controllers.ReviewController">
    <children>

        <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="53.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../utils/imgs/logo.png" />
            </image>
        </ImageView>
      <AnchorPane layoutX="31.0" layoutY="117.0" prefHeight="397.0" prefWidth="688.0" styleClass="sub-container">
         <children>
              <Button fx:id="editCollectionBtn" onAction="#handleAddButton" layoutX="33.0" layoutY="14.0" mnemonicParsing="false" styleClass="transparent-button" stylesheets="@../style/review.css" text="Edit your collections">
                  <graphic>
                      <ImageView fitHeight="48" fitWidth="48" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@../utils/icons/review/icons8-edit-property-48.png" />
                          </image>
                      </ImageView>
                  </graphic>
              </Button>
      
      
              <AnchorPane fx:id="reviewContainer" layoutY="71.0" prefHeight="360.0" prefWidth="688.0" />
              <Button fx:id="playBtn" layoutX="238.0" layoutY="14.0" mnemonicParsing="false" styleClass="transparent-button" stylesheets="@../style/review.css" text="Start studying">
               <graphic>
                      <ImageView fitHeight="48" fitWidth="48" pickOnBounds="true" preserveRatio="true">
                          <image>
                              <Image url="@../utils/icons/review/icons8-study-48.png" />
                          </image>
                      </ImageView>
               </graphic>
              </Button>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
