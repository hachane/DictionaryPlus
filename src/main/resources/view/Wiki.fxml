<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Button?>
<AnchorPane prefHeight="560.0" prefWidth="750.0" styleClass="main-container" stylesheets="@../style/main1.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dictionary.controllers.WikiController">
    <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="53.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@../utils/imgs/logo.png" />
        </image>
    </ImageView>
   <AnchorPane layoutX="189.0" layoutY="34.0" prefHeight="45.0" prefWidth="506.0" styleClass="sub-container">
      <children>
          <Button fx:id="search" onAction="#handleSearch" layoutX="1" layoutY="1" mnemonicParsing="false" styleClass="transparentButton">
              <graphic>
                  <ImageView fitHeight="48" fitWidth="48" pickOnBounds="true" preserveRatio="true">
                      <image>
                          <Image url="@../utils/icons/search/search.png" />
                      </image>
                  </ImageView>
              </graphic>
          </Button>
         <TextField fx:id="searchBox" layoutX="52.0" layoutY="10.0" prefHeight="29.0" prefWidth="432.0" styleClass="text-field" />
      </children>
   </AnchorPane>
   <AnchorPane layoutX="37.0" layoutY="108.0" prefHeight="423.0" prefWidth="683.0" styleClass="sub-container">
      <children>
         <TextArea fx:id="renderedText" layoutX="30.0" layoutY="23.0" prefHeight="378.0" prefWidth="625.0" styleClass="text-area" />
      </children>
   </AnchorPane>

</AnchorPane>
